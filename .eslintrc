{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "mocha": true
  },
  "rules": {
    "func-names": ["error", "never"],
    "comma-dangle": ["error", {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "never"
    }]
  }
}